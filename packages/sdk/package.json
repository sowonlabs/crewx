{
  "name": "@sowonai/crewx-sdk",
  "version": "0.7.8-rc.5",
  "license": "Apache-2.0",
  "description": "SowonAI CrewX SDK",
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "schema",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run --config ./vitest.config.ts",
    "test:watch": "vitest --config ./vitest.config.ts",
    "test:ui": "vitest --ui --config ./vitest.config.ts",
    "test:unit": "vitest run --config ./vitest.config.ts tests/unit/**/*.test.ts",
    "test:integration": "vitest run --config ./vitest.config.ts tests/integration/**/*.test.ts --passWithNoTests",
    "test:coverage": "vitest run --config ./vitest.config.ts --coverage",
    "lint": "echo \"SDK lint pending\" && exit 0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./internal": {
      "types": "./dist/internal/index.d.ts",
      "import": "./dist/internal/index.js",
      "require": "./dist/internal/index.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "import": "./dist/tools/index.js",
      "require": "./dist/tools/index.js"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "tools": ["./dist/tools/index.d.ts"],
      "internal": ["./dist/internal/index.d.ts"],
      "*": ["./dist/*"]
    }
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.8",
    "@ai-sdk/google": "^1.0.10",
    "@ai-sdk/openai": "^1.0.11",
    "@ai-sdk/openai-compatible": "^1.0.0",
    "@mastra/core": "^0.24.0",
    "@openrouter/ai-sdk-provider": "^1.2.2",
    "ai": "^5.0.92",
    "glob": "^11.0.0",
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "dotenv": "^17.2.3",
    "typescript": "^5.0.0"
  }
}
